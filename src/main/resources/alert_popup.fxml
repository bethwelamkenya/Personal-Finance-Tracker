<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="CENTER" spacing="20.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.AlertPopupController">
   <children>
      <Label fx:id="messageLabel" styleClass="form-label" text="Label">
         <VBox.margin>
            <Insets left="10.0" right="10.0" />
         </VBox.margin>
      </Label>
      <Button fx:id="okButton" mnemonicParsing="false" text="Ok">
         <VBox.margin>
            <Insets left="10.0" right="10.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
